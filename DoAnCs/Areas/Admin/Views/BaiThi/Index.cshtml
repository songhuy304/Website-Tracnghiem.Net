@model PagedList.PagedList<DoAnCs.Models.Exam_Results>
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />
@using PagedList.Mvc;
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_layout.cshtml";
}
@section naviheader{
    <h1>Quản Lý Bài Thi</h1>
    <ul class="breadcrumb">
        <li>
            <a href="#">Home</a>
        </li>
        <li><i class='bx bx-chevron-right'></i></li>
        <li>
            <a class="active" href="#">Quản Lý Bài Thi</a>
        </li>
    </ul>


}
<div class="order">

    @if (Model.Any() && Model != null)
    {
        var i = 1;
        <table>
            <thead>
                <tr>
                    <th>STT</th>
                    <th>Tên Bài Thi</th>
                    <th>Ngày Làm</th>
                    <th>Điểm Số</th>


                    <th class="float-right"></th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model)
                {

                    <tr>
                        <td>
                            <p>@i</p>
                        </td>
                        <td>@item.Exam.NameExam</td>
                        <td>@item.ExamDay</td>
                        <td>
                           @item.Score
                        </td>





                    </tr>

                    i++;
                }



            </tbody>
        </table>

    }
    else
    {
        <p>Không Có Gì</p>
    }
    @Html.PagedListPager(Model, page => Url.Action("LichSuThi",
                new { page }))
</div>

