
@model DoAnCs.Models.Viewmodel.ThongKeModel
@if (Model != null)
{
  <div class="order">
      <div class="head">
          <h3 class="text-center">Số lượt Truy cập</h3>
      </div>
      <div class="card-body">
          <table class="table table-bordered">
              <tr>
                  <td>Hôm nay</td>
                  <td>@Model.HomNay</td>
              </tr>
              <tr>
                  <td>Hôm qua</td>
                  <td>@Model.HomQua</td>
              </tr>
              <tr>
                  <td>Tuần này</td>
                  <td>@Model.TuanNay</td>
              </tr>
              <tr>
                  <td>Tất Cả</td>
                  <td>@Model.TatCa</td>
              </tr>
              <tr>
                  <td>Số người đang Online</td>
                  <td>@ViewBag.Visitors_online</td>
              </tr>
          </table>
      </div>
  </div>
}

